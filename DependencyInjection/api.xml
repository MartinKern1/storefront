<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service alias="Shopware\Storefront\Api\Seo\Repository\SeoUrlRepository.v1" id="Shopware\Storefront\Api\Seo\Repository\SeoUrlRepository" public="true"/>

        <service class="Shopware\Storefront\Api\Seo\Definition\SeoUrlDefinition" id="shopware.seo.seo_url_definition" >
            <tag name="shopware.entity.definition" entity="seo_url" />
        </service>

        <service class="Shopware\Storefront\Api\Seo\Repository\SeoUrlRepository" id="Shopware\Storefront\Api\Seo\Repository\SeoUrlRepository" public="true">
          <argument id="Shopware\Framework\ORM\Dbal\EntityReader" type="service"/>
          <argument id="Shopware\Version\VersionManager" type="service"/>
          <argument id="Shopware\Framework\ORM\Dbal\EntitySearcher" type="service"/>
          <argument id="Shopware\Framework\ORM\Dbal\EntityAggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                