{% block sw_product_detail_base_settings_card %}
    {% parent() %}

    {% block sw_product_detail_base_settings_seo_card %}
        <sw-seo-url v-if="product.extensions && product.extensions.seoUrls && product.extensions.seoUrls.length > 0"
                :isLoading="loading.product"
                :urls="product.extensions.seoUrls">

                <template #seo-additional="props">

                    {% block sw_product_detail_base_settings_seo_card_additions %}
                        <sw-seo-main-category :currentSalesChannelId="props.currentSalesChannelId"
                                              :categories="product.categories"
                                              :mainCategories="product.extensions.mainCategories"
                                              :isLoading="loading.product"
                                              @main-category-add="onMainCategoryAdded">
                        </sw-seo-main-category>
                    {% endblock %}

                </template>
        </sw-seo-url>
    {% endblock %}
{% endblock %}
