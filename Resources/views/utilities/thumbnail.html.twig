<picture class="{{ name }}"{% spaceless %}
        {% for key,value in attributes %}
            {{ key }}="{{ value }}"
        {% endfor %}
    {% endspaceless %}
    >
    {% for thumbnail in thumbnails %}
        <source srcset="{{ thumbnail.url }}" media="(max-width: {{ thumbnail.width }}px)" />
    {% endfor %}

    {% if default != '' %}
        {% set lastThumb = default %}
    {% else %}
        {% set lastThumb = thumbnails.last.url %}
    {% endif %}
    <img src="{{ lastThumb }}" alt="{{ alt }}" />
</picture>