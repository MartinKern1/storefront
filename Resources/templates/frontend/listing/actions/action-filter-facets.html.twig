{#  Iteration for the different filter facets.
    The file is called recursive for deeper structured facet groups.
#}
{% for facet in facets %}
    {# @var facet \Shopware\Storefront\Page\Listing\AggregationView\AggregationViewInterface #}
    {% if facet.type == 'slider' %}
        {% sw_include '@Storefront/frontend/listing/filter/facet-range.html.twig' with {'facet': facet} %}
    {% elseif facet.type == 'list' %}
        {% sw_include '@Storefront/frontend/listing/filter/facet-value-list.html.twig' with {'facet': facet} %}

    {% endif %}
{% endfor %}