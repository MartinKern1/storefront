{% block sw_seo_url_template_card %}
    <sw-card class="sw-seo-url-template-card"
             :title="$tc('sw-seo-url-template-card.general.titleCard')"
             :isLoading="isLoading">
        <template v-for="seoUrl in seoUrls">
            <sw-container columns="2fr 1fr" gap="5px">
                <sw-text-field :label="getLabel(seoUrl)"
                               v-model="seoUrl.template">
                </sw-text-field>
                <sw-select
                        :store="getEntityPropertyStore(seoUrl.entityName)"
                        :ref="`select-${seoUrl.entityName}`"
                        @input="onSelectInput($event, seoUrl)"
                        :label="$tc('sw-seo-url-template-card.general.labelPossibleValues')">

                </sw-select>
            </sw-container>
        </template>
    </sw-card>
{% endblock %}